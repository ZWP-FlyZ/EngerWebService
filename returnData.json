

//登录返回数据
{
	errCode//状态码
	token//授权码
	userInfo//用户其他信息
}

//登录包含的用户信息 UserInfo
{
	username//用户名
	userAuth//用户权限信息
	roleName//账号所属组织名，例如公司名，运管单位名，港行单位名，交通厅名
	roleType//账号所属组织类型，例如公司，运管单位，港行单位，交通厅，与roleName对应
	place1//市级地址
	place2//区县级地址

	////////还有其他需要数据
}



//注册返回数据
{
	errCode//状态码
}

//首页返回数据
{
	errCode//状态码
	roleName//角色名
	timeRange//统计时间范围
	xs://X轴类型或者饼图样例
	[
		[燃料类型1,燃料类型2.....]//燃料类型
		//......
	]

	engTypOther://每种燃料每个月使用量
	[
		{
			baseTyp//燃料类型
			engTypMo://该燃料每个月的使用量
			[
				{
					type//yyyy-mm类型
					TypDatOfAllEng//能耗总量
					TypDatOfAllLen//周转总量 ！！！！该数据无效
				}
			]//end Month
		}
	]

}//



//道路客运返回数据
{
	errCode//状态码
	roleName//角色名
	timeRange//统计时间范围
	xs://X轴类型或者饼图样例
	[
		[年月1,年月2.....]//年月类型
		[燃料类型1,燃料类型2.....]//燃料类型
		[客座类型1,客座类型2.....]//客座类型
		[企业规模类型1,企业规模类型2.....]//企业规模类型
		[运距类型1,运距规模类型2.....]//运距规模类型
		[车辆类型1,车辆规模类型2.....]//车辆规模类型
	]

	engTypOther://每种燃料与其他条件
	[
		{
			baseTyp//燃料类型

			engTypMo://该燃料每个月的使用量
			[
				{
					type//yyyy-mm类型
					TypDatOfAllEng//能耗总量
					TypDatOfAllLen//周转总量
				}
			]//end Month

			engTypSs://该燃料不同客座类型数据
			[
				{
					type//类型
					TypDatOfAllEng//能耗总量
					TypDatOfAllLen//周转总量
				}
			]//end SS			
		}//end EngType		
	]

	entTypOther://企业类型其他
	[
		{
			baseTyp//企业类型
			baseTypDatOfAllEng//能耗总量
			baseTypDatOfAllLen//周转总量
		}
	]

	disTypOther://运距类型其他
	[
		{
			baseTyp//运距类型
			baseTypDatOfAllEng//能耗总量
			entTypDatOfAllLen//周转总量
		}
	]

	carTypOther://车辆类型其他
	[
		{
			baseTyp//车辆类型
			baseTypDatOfAllEng//能耗总量
			baseTypDatOfAllLen//周转总量
		}
	]


}





